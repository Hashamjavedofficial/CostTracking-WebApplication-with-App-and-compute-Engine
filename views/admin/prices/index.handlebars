<h1>Prices of the dealers</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Seller</th>
            <th scope="col">Price</th>
            <th scope="col">Date</th>
        </tr>
    </thead>
    <tbody>
        {{#each prices}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{user.name}}</td>
            <td>{{prices}}</td>
            <td>{{generateTime date 'MMM Do YYYY'}}</td>

            <td>
                <form action="/admin/prices/{{id}}?_method=DELETE" method="post">
                    <button type="submit" class="btn btn-danger">Delete</button>
                </form>

            </td>

        </tr>

        {{/each}}
    </tbody>
</table>