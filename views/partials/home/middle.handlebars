<h2 class="center">Mobile Category</h2>
<br>

<div class="row ">
    {{#each categories}}
    <div class="col-lg-3"><br />
        <div class="box text-center" data-aos="fade-left" data-aos-duration="1300">
            <div>
                <img src="/uploads/{{file}}" width="110px" height="100px" />
            </div>
            <br />
            <p><strong>Company:</strong> {{name}}</p>
            <hr />


            <a href="/phone/{{id}}" class="btn btn-sm btn-primary">More</a>



            <br />
            <br />
        </div>


    </div>

    {{/each}}


